Options +FollowSymlinks
RewriteEngine On
RewriteBase /app/plugin/agapesHotes/page/
RewriteRule ^allSecteurs/$  allSecteurs.php [L]
RewriteRule ^allSites/$  allSites.php [L]
RewriteRule ^allEtablissements/$  allEtablissements.php [L]
RewriteRule ^allEmployes/$  allEmployes.php [L]
RewriteRule ^allAccess/$  allAccess.php [L]
RewriteRule ^allPti/$  allPti.php [L]
RewriteRule ^allPrestations/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/$  allPrestations.php?secteur=$1&site=$2 [NC,L]
RewriteRule ^mainCourante/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/$  updateMainCourante.php?secteur=$1&site=$2 [NC,L]
RewriteRule ^mainSupplementaire/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/$  updateMainSupplementaire.php?secteur=$1&site=$2 [NC,L]
RewriteRule ^allCourses/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/$  allCourses.php?secteur=$1&site=$2&etablissement=$3 [NC,L]
RewriteRule ^vivreCrue/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/([a-zA-Z0-9-]+)/$  updateVivreCrue.php?secteur=$1&site=$2&etablissement=$3 [NC,L]

RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule (.*) /404.php